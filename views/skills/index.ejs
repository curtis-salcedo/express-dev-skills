<%- include('../partials/header') %>


<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <a href="/skills/<%= s.id %>"><%= s.skill %></a>
      - 
      <%= s.done ? 'done' : 'not done' %>
    </li>
    <% }); %>
  </ul>
  <form action="/skills/new">
    <button type="submit">Add Skill</button>
  </form>
<%- include('../partials/footer') %>